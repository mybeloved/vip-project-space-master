<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="gpmall-cashier" owner="mic"/>

    <dubbo:registry protocol="zookeeper" group="dubbo-dev" address="192.168.8.126:2181"/>

    <import resource="classpath*:tcc-transaction.xml"/>
    <import resource="classpath*:tcc-transaction-dubbo.xml"/>
    <import resource="classpath*:client/user-reference.xml"/>
    <import resource="classpath*:client/pay-reference.xml"/>
    <import resource="classpath*:client/tcc-order-reference.xml"/>

</beans>